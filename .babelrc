{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": 
        [
          "transform-react-jsx-source",
          ["module-resolver",{
            "cwd": "babelrc",
              "root": ["./src"],
              "alias": {
                "assets": "./assets",
                "components": "./components",
                "core-components":"./components/core-components",
                "styles":"./styles",
                "data":"./data",
                "env":"./env"
              }
            }
          ]
        ]
      }
    }
  }